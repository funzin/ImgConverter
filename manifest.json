{
  "name": "ConvertToImageTag",
  "version": "0.1",
  "description": "convert to image tag",
  "manifest_version": 2,
  "permissions": [
    "activeTab",
    "storage"
  ],
  "background": {
    "scripts": [
      "src/lib/jquery-3.4.1.min.js",
      "src/js/background.js"
    ],
    "persistant": false
  },
  "browser_action": {
    "name": "convert"
  },
  "options_page": "src/html/options.html"
}